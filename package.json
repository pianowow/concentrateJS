{
   "name": "concentrate",
   "version": "0.0.0",
   "private": true,
   "type": "module",
   "scripts": {
      "dev": "vite",
      "build": "vue-tsc --noEmit -p tsconfig.json && vite build",
      "preview": "vite preview",
      "lint": "eslint . --fix",
      "test": "vitest",
      "bench": "vitest bench --run",
      "test:watch": "vitest --watch",
      "test:ui": "vitest --ui",
      "typecheck": "vue-tsc --noEmit -p tsconfig.json",
      "typecheck:watch": "vue-tsc --noEmit -w -p tsconfig.json"
   },
   "dependencies": {
      "@vueuse/core": "^14.1.0",
      "pinia": "^3.0.4",
      "vue": "^3.5.17",
      "vue-router": "^4.6.4"
   },
   "devDependencies": {
      "@eslint/js": "^9.29.0",
      "@types/node": "^24.10.0",
      "@vitejs/plugin-vue": "^6.0.0",
      "@vitest/ui": "^4.0.6",
      "@vue/tsconfig": "^0.8.1",
      "eslint": "^9.29.0",
      "eslint-plugin-vue": "~10.2.0",
      "globals": "^16.2.0",
      "prettier": "^3.6.2",
      "typescript": "^5.9.3",
      "typescript-eslint": "^8.7.0",
      "vite": "^7.0.0",
      "vite-plugin-vue-devtools": "^7.7.7",
      "vitest": "^4.0.6",
      "vue-tsc": "^3.1.3"
   }
}
